@model Technician
<h3>Delete Technician</h3>

<p>
    Technician Name: @Model.technicianFullName
    <br />
    Technician Email: @Model.technicianEmail
    <br />
    Technician Phone: @Model.technicianPhone
</p>
<p>Are you sure you want to delete this technician?</p>
<form asp-action="Delete" asp-controller="Technician" method="post">
    <input type="hidden" value="@Model.technicianId" asp-for="technicianId" />
    <input type="hidden" id="technicianFullName" name="technicianFullName" value="@Model.technicianFullName" asp-for="technicianFullName" />
    <input type="hidden" id="technicianEmail" name="technicianEmail" value="@Model.technicianEmail" asp-for="technicianEmail" />
    <input type="hidden" id="technicianPhone" name="technicianPhone" value="@Model.technicianPhone" asp-for="technicianPhone" />
    <input type="submit" value="Delete" />
    <button type="button" onclick="location.href='@Url.Action("List", "Technician")'">Cancel</button>
</form>